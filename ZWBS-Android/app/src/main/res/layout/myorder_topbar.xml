            android:background="@color/lonincolors" />
    </RelativeLayout>


    <RelativeLayout style="@style/AutoMatch">

        <TextView
            android:id="@+id/tv_inDistribution"
            style="@style/MatchMatch"
            android:gravity="center"
            android:text="@string/inDistribution"
