            android:background="@color/lonincolors" />
    </RelativeLayout>


    <RelativeLayout style="@style/AutoMatch">

        <TextView
            android:id="@+id/tv_distributionComplete"
            style="@style/MatchMatch"
            android:gravity="center"
            android:text="@string/distributionComplete"
            android:textColor="@color/lonincolors"
            android:textSize="@dimen/textsize13" />

        <TextView
