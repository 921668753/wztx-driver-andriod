                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/models"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_models"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen70"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/load"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_load"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen70"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/carrierProduct"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_carrierProduct"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen41"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>


            <com.ruitukeji.zwbs.utils.myview.ChildLiistView
                android:id="@+id/lv_img"
                style="@style/MatchWrap"
                android:layout_marginTop="@dimen/dimen15"
                android:background="@color/mainColor">

            </com.ruitukeji.zwbs.utils.myview.ChildLiistView>

        </LinearLayout>
    </ScrollView>
