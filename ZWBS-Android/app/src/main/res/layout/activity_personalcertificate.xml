<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:background="@color/background"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <include layout="@layout/common_title"></include>

    <ScrollView
        style="@style/MatchAuto"
        android:scrollbars="none">

        <LinearLayout
            style="@style/MatchWrap"
            android:orientation="vertical">

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/realName"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_realName"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen41"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/bindingMobilePhone"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_bindingMobilePhone"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen26"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/IdNumber"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_idNumber"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen41"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/sex"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_sex"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen70"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>

            <LinearLayout
                style="@style/Match44"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/permanentAddress"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

                <TextView
                    android:id="@+id/tv_permanentAddress"
                    style="@style/AutoWrap"
                    android:layout_marginLeft="@dimen/dimen41"
                    android:layout_marginRight="@dimen/dimen15"
                    android:text="--"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/textsize15" />
            </LinearLayout>


            <LinearLayout
                style="@style/Match44"
                android:layout_marginTop="@dimen/dimen15"
                android:background="@color/mainColor"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginLeft="@dimen/dimen15"
                    android:gravity="center"
                    android:text="@string/licenseNumber1"
                    android:textColor="@color/hintcolors"
                    android:textSize="@dimen/textsize15" />

