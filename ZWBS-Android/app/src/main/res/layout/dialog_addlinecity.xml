<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/MatchMatch"
    android:layout_marginTop="@dimen/dimen165"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView style="@style/DividerWidth" />

    <LinearLayout
        style="@style/MatchWrap"
        android:orientation="horizontal">

        <TextView
            style="@style/WrapWrap"
            android:paddingBottom="@dimen/dimen13"
            android:paddingLeft="@dimen/dimen10"
            android:paddingTop="@dimen/dimen13"
            android:text="@string/currentRegion"
            android:textColor="@color/titletextcolors"
            android:textSize="@dimen/textsize14" />

        <TextView
            android:id="@+id/tv_select"
            style="@style/WrapWrap"
            android:paddingBottom="@dimen/dimen13"
            android:paddingTop="@dimen/dimen13"
            android:text="--"
            android:textColor="@color/titletextcolors"
            android:textSize="@dimen/textsize14" />

        <TextView style="@style/AutoMatch" />

        <LinearLayout
            android:id="@+id/ll_back"
            style="@style/WrapMatch"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dimen17"
                android:layout_height="@dimen/dimen17"
                android:src="@mipmap/backline" />

            <TextView
                style="@style/WrapMatch"
                android:layout_marginLeft="@dimen/dimen7"
                android:gravity="center"
                android:paddingLeft="@dimen/dimen1"
                android:paddingRight="@dimen/dimen10"
                android:text="@string/returnUpperLevel"
                android:textColor="@color/announcementCloseColors"
                android:textSize="@dimen/textsize14" />

        </LinearLayout>


    </LinearLayout>


    <GridView
        android:id="@+id/gv_address"
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/dimen10"
        android:layout_marginRight="@dimen/dimen10"
        android:listSelector="@android:color/transparent"
        android:numColumns="4"
        android:overScrollMode="never"
        android:paddingBottom="@dimen/dimen10"
        android:scrollbars="none"
        tools:listitem="@layout/item_addlineaddress" />


</LinearLayout>