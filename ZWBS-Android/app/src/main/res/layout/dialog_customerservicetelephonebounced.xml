<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:background="@color/transparent_1"
    android:gravity="center"
    android:orientation="vertical">


    <LinearLayout
        style="@style/MatchWrap"
        android:layout_margin="@dimen/dimen40"
        android:background="@drawable/shape_sendquotationbounced"
        android:gravity="center"
        android:orientation="vertical">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen90"
            android:gravity="center"
            android:text="400-123-456"
            android:textColor="@color/f2222Colors"
            android:textSize="@dimen/textsize18" />


        <TextView style="@style/DividerWidth" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tv_cancel"
                style="@style/AutoWrap"
                android:gravity="center"
                android:paddingBottom="@dimen/dimen15"
                android:paddingTop="@dimen/dimen15"
                android:text="@string/cancel1"
                android:textColor="@color/typecolors"
                android:textSize="@dimen/textsize15" />


            <TextView style="@style/DividerHeight" />


            <TextView
                android:id="@+id/tv_determine"
                style="@style/AutoWrap"
                android:gravity="center"
                android:paddingBottom="@dimen/dimen15"
                android:paddingTop="@dimen/dimen15"
                android:text="@string/determine"
                android:textColor="@color/aFF007Colors"
                android:textSize="@dimen/textsize15" />


        </LinearLayout>


    </LinearLayout>

</LinearLayout>